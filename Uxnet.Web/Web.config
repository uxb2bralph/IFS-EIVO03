      </setting>
      <setting name="HostUrl" serializeAs="String">
        <value>https://eivo.uxifs.com</value>
      </setting>
      <setting name="WebMaster" serializeAs="String">
        <value>網際優勢 &lt;invoice_services05@uxb2b.com&gt;</value>
      </setting>
    </Uxnet.Web.Properties.Settings>
